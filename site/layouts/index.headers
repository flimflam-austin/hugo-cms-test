/*
  Strict-Transport-Security: max-age=31536000; includeSubDomains; preload
  X-Content-Type-Options: nosniff
  X-XSS-Protection: 1; mode=block
  Content-Security-Policy: default-src 'self'; manifest-src 'self'; connect-src 'self'; font-src 'fonts.gstatic.com'; img-src *; media-src *; script-src 'self'; style-src 'self'; frame-src: 'self'; child-src https://www.youtube.com https://player.vimeo.com
  X-Frame-Options: SAMEORIGIN
  Referrer-Policy: strict-origin
  Feature-Policy: geolocation 'self'; microphone 'none'; camera 'none'
  Cache-Control: public, max-age=31536000
